<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity2"
    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:orientation="vertical"
        android:background="@drawable/back31_">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="?actionBarSize"
            android:background="#833F51B5"
            android:backgroundTint="#833F51B5">

            <TextView
                android:id="@+id/welcome"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="165dp"
                android:layout_marginEnd="19dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/aclonica"
                android:gravity="center_vertical"
                android:text=""
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="63dp"
                android:layout_marginEnd="19dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/aclonica"
                android:gravity="center_vertical"
                android:text="Welcome"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/null1"
                android:layout_width="50dp"
                android:layout_height="50dp"

                android:layout_alignParentRight="true"
                android:src="@drawable/ic_baseline_exit_to_app_24">

            </ImageView>

            <ImageView
                android:id="@+id/person"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignBottom="@+id/null1"


                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="1dp"
                android:layout_marginTop="1dp"

                android:layout_marginBottom="-1dp"


                android:src="@drawable/ic_baseline_account_circle_24">

            </ImageView>


        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aclonica"
            android:padding="10dp"
            android:text="Categories"
            android:textAllCaps="true"
            android:textColor="@color/colorWhite"
            android:textSize="30sp" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/border"
                android:layout_margin="30dp">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/aclonica"
                    android:padding="10dp"
                    android:paddingStart="30dp"
                    android:text="Rank


                    Predictor"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite"
                    android:textSize="25sp" />

                <Button
                    android:id="@+id/rank"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="true"
                    android:background="#00ff0000"
                    android:padding="20dp"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite" />



            </RelativeLayout>

            <ImageView
                android:layout_width="75dp"
                android:layout_height="85dp"
                android:layout_marginLeft="225dp"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/ranking" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/border"
                android:layout_margin="30dp">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/aclonica"
                    android:padding="10dp"
                    android:paddingStart="30dp"
                    android:text="College


                    Predictor"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite"
                    android:textSize="25sp" />

                <Button
                    android:id="@+id/college"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="true"
                    android:background="#00ff0000"
                    android:padding="20dp"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite" />



            </RelativeLayout>

            <ImageView
                android:layout_width="75dp"
                android:layout_height="85dp"
                android:layout_marginLeft="230dp"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/school" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/border"
                android:layout_margin="30dp">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/aclonica"
                    android:padding="10dp"
                    android:paddingStart="30dp"
                    android:text="Video


                    Lectures"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite"
                    android:textSize="25sp" />

                <Button
                    android:id="@+id/videos"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="true"
                    android:background="#00ff0000"
                    android:padding="20dp"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite" />



            </RelativeLayout>

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="225dp"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/videos" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/border"
                android:layout_margin="30dp">


                <TextView
                    android:id="@+id/previous"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/aclonica"
                    android:padding="10dp"
                    android:paddingStart="25dp"
                    android:text="previous
                     year papers"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite"
                    android:textSize="25sp" />


            </RelativeLayout>

            <ImageView
                android:layout_width="75dp"
                android:layout_height="85dp"
                android:layout_marginLeft="240dp"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/exam" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/border"
                android:layout_margin="30dp">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/aclonica"
                    android:padding="20dp"
                    android:text="MOck test"
                    android:textAllCaps="true"
                    android:paddingStart="30dp"
                    android:textColor="@color/colorWhite"
                    android:textSize="25sp" />

                <Button

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="true"
                    android:background="#00ff0000"
                    android:padding="20dp"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite" />



            </RelativeLayout>

            <ImageView
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginLeft="240dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/test"
                android:layout_marginTop="28dp"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/border"
                android:layout_margin="30dp">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/aclonica"
                    android:padding="10dp"
                    android:text="college
                    list"
                    android:textAllCaps="true"
                    android:paddingStart="30dp"
                    android:textColor="@color/colorWhite"
                    android:textSize="25sp" />



                <Button
                    android:id="@+id/collegeBTN"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="-270dp"
                    android:background="#00ff0000"
                    android:padding="20dp"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite" />


            </RelativeLayout>

            <ImageView
                android:layout_width="75dp"
                android:layout_height="85dp"
                android:layout_marginLeft="230dp"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/school" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/border"
                android:layout_margin="30dp">


                <TextView
                    android:layout_width="225dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/aclonica"
                    android:padding="10dp"
                    android:paddingStart="30dp"
                    android:text="all about
kcet exam"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite"
                    android:textSize="25sp" />

                <Button
                    android:id="@+id/doc"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="true"
                    android:background="#00ff0000"
                    android:padding="25dp"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite" />

                <Button
                    android:id="@+id/d"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="-270dp"
                    android:background="#00ff0000"
                    android:padding="20dp"
                    android:textAllCaps="true"
                    android:textColor="@color/colorWhite" />


            </RelativeLayout>

            <ImageView
                android:layout_width="75dp"
                android:layout_height="85dp"
                android:layout_marginLeft="240dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/documents"
                android:layout_marginTop="28dp"/>
        </RelativeLayout>

        <com.google.android.material.bottomnavigation.BottomNavigationView
            android:id="@+id/navigation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/border"
            android:layout_alignParentBottom="true"
            app:menu="@menu/menu"
            />

    </LinearLayout>


</ScrollView>